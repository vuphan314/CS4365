define T(n, x)
    return
        2 if n = 0 else
        x if n = 1 else
        x * T(n - 1, x) - T(n - 2, x)

define curve(a, b)
    return
        "the curve with slope a/b, as a variable, say C"

define skein(p, q)
    let n be gcd(p, q)
    let p' be p / n
    let q' be q / n
    let C be curve(p', q')
    return T(n, C)
