define T(n, x)
    return
        2 if n = 0 else
        x if n = 1 else
        x * T(n - 1, x) - T(n - 2, x)

define skein(p, q)
    let n be gcd(p, q)
    let p2 be p / n
    let q2 be q / n
    let c be SCC(p2, q2)
    return T(n, c)

vary x

check T2x print T(2, x)

check skeinP2Q2 print skein(2, 2)
